<Application x:Class="Wrap.Unpackager.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Wrap.Unpackager"
             StartupUri="Views/MainView.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <!--WPF-UI Resources-->
                <ResourceDictionary Source="pack://application:,,,/Wpf.Ui;component/Styles/Theme/Dark.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Wpf.Ui;component/Styles/Wpf.Ui.xaml" />

                <!--Default Assets-->
                <ResourceDictionary Source="pack://application:,,,/WRAP.Unpackager;component/Assets/Converters.xaml" />
                <ResourceDictionary Source="pack://application:,,,/WRAP.Unpackager;component/Assets/Localizations/Localization.en.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
